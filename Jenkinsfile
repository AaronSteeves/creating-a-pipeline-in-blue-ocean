pipeline {
  agent any
  stages {
    stage('Start') {
      steps {
        echo 'Starting'
      }
    }

    stage('Test') {
      steps {
        echo 'run Tests'
      }
    }

    stage('Deploy') {
      steps {
        echo 'Deploy'
      }
    }

    stage('Done') {
      steps {
        echo 'Bye'
      }
    }

  }
}